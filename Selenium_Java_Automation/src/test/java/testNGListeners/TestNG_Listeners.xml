<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="TestNGListenerSuite">
	<listeners>
		<listener class-name="testNGListeners.Listeners" />
	</listeners>
	
	<test name="LoginTest">
		<classes>
			<class name="testNGListeners.LoginTest">
			<methods>
			  <include name="loginByEmail"/>
			  <exclude name="loginByFacebook"/> 
			  <include name="loginByName"/>
			</methods>
			</class>
		</classes>
	</test>
</suite>
<!-- Suite -->